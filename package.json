{
  "name": "paste-hub",
  "version": "1.0.0",
  "main": "dist/server",
  "scripts": {
    "client:dev": "vite src/client",
    "client:build": "vite build src/client --outDir=../../dist/client",
    "preview": "vite preview",
    "server:dev": "nodemon src/server",
    "server:build": "esbuild ./src/server --platform=node --bundle --minify --outfile=dist/server.js",
    "lint": "yarn eslint src",
    "dev": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
    "build": "yarn server:build && yarn client:build",
    "start": "node dist/server"
  },
  "repository": "git@github.com:sunny775/paste-hub.git",
  "author": "Sunday Nkwuda <sonnietech@disroot.org>",
  "license": "MIT",
  "dependencies": {
    "@monaco-editor/react": "^4.4.5",
    "@reach/visually-hidden": "^0.17.0",
    "aws-sdk": "^2.1165.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "monaco-editor": "^0.33.0",
    "nanoid": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-monaco-editor": "^0.48.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.3.0",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.2.2",
    "esbuild": "^0.14.43",
    "eslint": "^8.18.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.30.1",
    "nodemon": "^2.0.16",
    "vite": "^2.9.9"
  }
}
